{
  "name": "intus.drawing.frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "dev": "ng serve --configuration=development --open",
    "start:windows": "ng serve --open --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run Intus.Drawing.FrontEnd:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.7",
    "@angular/material": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/platform-server": "^15.2.7",
    "@angular/router": "^15.2.7",
    "chart.js": "^2.8.0",
    "chartjs-plugin-datalabels": "^0.7.0",
    "hammerjs": "2.0.8",
    "javascript-time-ago": "^2.0.7",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
    "ng2-charts": "^2.4.2",
    "ngx-toastr": "^15.2.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "^15.2.7",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/language-service": "^15.2.7",
    "@types/hammerjs": "^2.0.45",
    "@types/jasmine": "^3.5.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/javascript-time-ago": "^2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.2",
    "core-js": "^3.6.4",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "run-script-os": "^1.1.6",
    "typescript": "~4.9.5"
  },
  "optionalDependencies": {
    "node-sass": "^9.0.0",
    "ts-node": "~5.0.1"
  }
}
