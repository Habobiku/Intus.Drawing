{
  "compileOnSave": false,
  "angularCompilerOptions": {
  },
    "compilerOptions": {
      "baseUrl": "./src",
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "module": "es2020",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "allowUnusedLabels": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "strictNullChecks": false,
      "target": "es2020",
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "es2018",
        "dom"
      ],
      "paths": {
        "@services/*": [ "app/@services/*" ],
        "@models/*": [ "app/@models/*" ],
        "@core/*": [ "app/core/*" ],
        "@utils": [ "app/shared/utils/utils" ],
        "@utils/*": [ "app/shared/utils/*" ],
        "@shared": [ "app/shared/shared.module" ],
        "@shared/*": [ "app/shared/*" ]
      }
    }
  }
